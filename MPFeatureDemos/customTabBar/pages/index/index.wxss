scroll-view::-webkit-scrollbar {
  display: none;
  width: 0;
  height: 0;
  color: transparent;
}

.scroll-container {
  height: 100vh;
  overflow: hidden;
  /* Android hide scroll*/
}

.container {
  padding: 20rpx 40rpx 110px 40rpx;
}

.container .corner {
  border-radius: 10px;
  width: 100%;
  height: 120rpx;
}